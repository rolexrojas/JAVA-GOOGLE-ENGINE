<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>EmpleadoServlet</servlet-name>
        <servlet-class>com.ReminderApp.Servlets.EmpleadoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewerServlet</servlet-name>
        <servlet-class>com.ReminderApp.Servlets.ViewerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmpleadoEditServlet</servlet-name>
        <servlet-class>com.ReminderApp.Servlets.EmpleadoEditServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EmpleadoServlet</servlet-name>
        <url-pattern>/EmpleadoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewerServlet</servlet-name>
        <url-pattern>/ViewerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmpleadoEditServlet</servlet-name>
        <url-pattern>/EmpleadoEditServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>15</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
